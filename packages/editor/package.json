{
  "name": "@enpage/editor",
  "version": "0.0.37",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/library.js",
      "types": "./dist/library.d.ts"
    },
    "./dist/assets/*.css": {
      "import": "./dist/assets/*.css"
    }
  },
  "scripts": {
    "dev": "node server",
    "build:watch": "vite build --watch",
