{
  "name": "@enpage/sdk",
  "version": "0.0.13",
  "type": "module",
  "files": [
    "dist",
    "src",
    "env.d.ts",
    "custom-attrs.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/sdk/sdk.js",
      "types": "./dist/sdk/sdk.d.ts"
    },
    "./env.d.ts": {
      "require": "./env.d.ts",
      "import": "./env.d.ts",
      "types": "./env.d.ts"
    },
    "./custom-attrs.d.ts": "./custom-attrs.d.ts",
    "./builder/*": {
      "import": "./dist/builder/*.js"
    },
    "./*": {
      "import": "./dist/sdk/*.js",
      "types": "./dist/sdk/*.d.ts"
    }
  },
  "bin": {
    "enpage": "dist/builder/node/cli.js"
  },
  "dependencies": {
    "@lit/reactive-element": "2.0.4",
    "chalk": "5.3.0",
    "clsx": "2.1.1",
    "commander": "12.1.0",
    "html-minifier": "4.0.0",
    "immer": "10.1.1",
    "jsdom": "24.1.0",
    "liquidjs": "10.14.0",
    "lit": "3.1.4",
    "lit-element": "4.0.6",
    "lit-html": "3.1.4",
    "nanoid": "5.0.7",
    "tiny-invariant": "1.3.3",
    "zod": "workspace:*"
  },
  "peerDependencies": {
    "@enpage/style-system": "workspace:*",
    "@enpage/types": "workspace:*"
  },
  "devDependencies": {
    "@types/html-minifier": "4.0.5",
    "@types/jsdom": "21.1.7",
    "@types/node": "^20.12.12",
    "axe-core": "4.9.1",
    "concurrently": "8.2.2",
    "cssnano": "7.0.4",
    "csstype": "3.1.3",
    "postcss-import": "16.1.0",
    "postcss-preset-env": "9.6.0",
    "rollup-plugin-visualizer": "5.12.0",
    "tailwindcss": "3.4.3",
    "tsup": "8.0.2",
    "vite": "5.2.11",
    "vite-plugin-dts": "workspace:*",
    "vite-tsconfig-paths": "workspace:*"
  },
  "scripts": {
    "build": "pnpm run build:builder && pnpm run build:sdk",
    "build:sdk": "vite build",
    "build:builder": "tsup",
    "dev:sdk": "vite build --watch",
    "dev:builder": "tsup --watch",
    "dev": "concurrently \"npm:dev:*\"",
    "lint": "nx eslint:lint",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Matthias Etienne",
  "publishConfig": {
    "access": "public"
  }
}
