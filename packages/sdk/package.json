{
  "name": "@upstart.gg/sdk",
  "version": "0.0.78",
  "type": "module",
  "files": [
    "dist",
    "env.d.ts"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/shared/index.js",
      "types": "./dist/shared/index.d.ts"
    },
    "./shared/*": {
      "types": "./dist/shared/*.d.ts",
      "import": "./dist/shared/*.js"
    },
    "./builder/*": {
      "import": "./dist/node/builder/*.js"
    },
    "./cli/*": {
      "import": "./dist/node/cli/*.js"
    },
    "./env.d.ts": {
      "types": "./env.d.ts",
      "require": "./env.d.ts",
      "import": "./env.d.ts"
    },
    "./*": {
      "types": "./dist/shared/*.d.ts",
      "import": "./dist/shared/*.js"
    }
  },
  "bin": {
    "enpage": "dist/node/cli/program.js"
  },
  "dependencies": {
