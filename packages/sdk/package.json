{
  "name": "@enpage/sdk",
  "version": "0.0.13",
  "type": "module",
  "files": [
    "dist",
    "src",
    "env.d.ts",
    "custom-attrs.d.ts"
  ],
  "exports": {
    "./browser/*": {
      "import": "./dist/browser/*.js",
      "types": "./dist/browser/*.d.ts"
    },
    "./builder/*": {
      "import": "./dist/node/builder/*.js"
    },
    "./cli/*": {
      "import": "./dist/node/cli/*.js"
    },
    "./env.d.ts": {
      "require": "./env.d.ts",
      "import": "./env.d.ts",
      "types": "./env.d.ts"
    },
    "./custom-attrs.d.ts": "./custom-attrs.d.ts",
    "./*": {
      "import": "./dist/shared/*.js",
      "types": "./dist/shared/*.d.ts"
    }
  },
  "bin": {
    "enpage": "dist/node/cli/program.js"
  },
  "dependencies": {
    "chalk": "5.3.0",
    "clsx": "2.1.1",
    "commander": "12.1.0",
    "html-minifier": "4.0.0",
    "immer": "10.1.1",
    "jsdom": "24.1.0",
    "liquidjs": "10.15.0",
    "nanoid": "5.0.7",
    "tiny-invariant": "1.3.3",
    "zod": "workspace:*"
  },
  "peerDependencies": {
    "@enpage/style-system": "workspace:*",
    "lodash-es": "4.17.21"
  },
  "devDependencies": {
    "@types/html-minifier": "4.0.5",
    "@types/jsdom": "21.1.7",
    "@types/lodash-es": "4.17.12",
    "@types/node": "^20.14.10",
    "autoprefixer": "10.4.19",
    "axe-core": "4.9.1",
    "concurrently": "8.2.2",
    "cssnano": "7.0.4",
    "csstype": "3.1.3",
    "lodash-es": "4.17.21",
    "postcss": "8.4.39",
    "postcss-import": "16.1.0",
    "postcss-nested": "6.0.1",
    "postcss-preset-env": "9.6.0",
    "rollup-plugin-visualizer": "5.12.0",
    "tailwindcss": "3.4.4",
    "tsup": "8.1.0",
    "vite": "5.2.11",
    "vite-plugin-dts": "workspace:*",
    "vite-tsconfig-paths": "workspace:*"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "biome check --write .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Matthias Etienne",
  "publishConfig": {
    "access": "public"
  }
}
