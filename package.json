{
  "name": "enpage-sdk-monorepo",
  "version": "1.0.0",
  "description": "The Enpage SDK is a library that provides the core functionality for the Enpage templates. It is developed using React, Vanilla Extract, and TypeScript.",
  "main": "eslint.config.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "pnpm build && pnpm --reporter-hide-prefix --parallel -r dev",
    "build": "pnpm exec nx run-many -t build",
    "lint": "pnpm --parallel --recursive lint",
    "ci:lint": "pnpm --parallel --recursive ci:lint",
    "changeset": "changeset",
    "publish:packages": "changeset publish",
    "version:packages": "changeset version",
    "docs:dev": "vitepress dev docs --port 8965",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:pdf": "press-export-pdf export docs --outFile docs.pdf"
  },
  "keywords": [],
  "author": "",
  "license": "AGPL-3.0-only",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "pnpm": {
    "overrides": {
      "@vitejs/plugin-react": "4.2.1",
      "typescript": "5.4.5",
      "react": "18.3.1",
      "react-dom": "18.3.1",
      "@types/react": "18.3.2",
      "@types/react-dom": "18.3.0",
      "@types/node": "^20.0.0",
      "goober": "2.1.14",
      "zod": "3.23.8",
      "tailwindcss": "3.4.3",
      "@tailwindcss/container-queries": "0.1.1",
      "@tailwindcss/forms": "0.5.7",
      "tailwind-scrollbar": "3.1.0",
      "vite": "5.2.11",
      "vite-plugin-dts": "3.9.1",
      "vite-tsconfig-paths": "4.3.2"
    }
  },
  "devDependencies": {
    "@changesets/cli": "2.27.5",
    "@biomejs/biome": "1.8.1",
    "@eslint/js": "9.4.0",
    "@nx/eslint": "19",
    "@nx/vite": "19",
    "@nx/web": "19",
    "@types/eslint__js": "8.42.3",
    "@types/node": "^20.12.12",
    "@vitest/ui": "^1.3.1",
    "eslint": "8.57.0",
    "nx": "19",
    "prettier": "3.2.5",
    "prettier-plugin-classnames": "0.6.6",
    "prettier-plugin-tailwindcss": "0.5.14",
    "tailwindcss": "*",
    "typescript": "latest",
    "typescript-eslint": "7.13.0",
    "vite": "^5.0.0",
    "vitepress": "1.2.3",
    "vitepress-export-pdf": "1.0.0",
    "vitest": "^1.3.1",
    "vue": "3.4.30"
  }
}
