{
  "name": "upstart",
  "version": "0.0.1",
  "description": "Monorepo for the Upstart SDK",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "dev": "pnpm build && pnpm --reporter-hide-prefix --parallel --filter=!@upstart.gg/template-example -r dev ",
    "build": "nx run-many -t build --maxParallel=3 --exclude=@upstart.gg/template-example --output-style stream",
    "lint": "pnpm --parallel --recursive lint",
    "ci:lint": "biome ci --changed --no-errors-on-unmatched --since=head --reporter=github --colors=off packages && pnpm --parallel --recursive ci:lint",
    "test-create-enpage": "rimraf testing/my-template && pnpm create enpage@latest testing/my-template",
    "changeset": "changeset",
    "clean": "pnpm -r exec -- rimraf node_modules && pnpm install",
    "publish:packages": "changeset publish",
    "version:packages": "changeset version",
    "docs:dev": "vitepress dev docs --port 8965",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [],
  "author": "Matthias Etienne",
  "license": "AGPL-3.0-only",
  "private": true,
  "engines": {
    "pnpm": ">=8.0.0",
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "2.27.8",
    "@types/node": "^22.4.0",
    "@vitest/ui": "2.0.5",
    "autoprefixer": "10.4.20",
    "dotenv": "16.4.5",
    "json-schema-to-markdown": "1.1.1",
    "nx": "19.7.2",
    "postcss": "8.4.42",
    "rimraf": "6.0.1",
    "tailwindcss": "3.4.10",
    "tsx": "4.19.0",
    "typescript": "5.5.3",
    "vite-tsconfig-paths": "5.0.1",
    "vitepress": "1.3.4",
    "vitest": "2.0.5",
    "vue": "3.5.4"
  },
  "pnpm": {
    "overrides": {
      "react": "18.2.0",
      "react-dom": "18.2.0",
      "@twind/core": "1.1.3",
      "@twind/with-react": "1.1.3",
      "@sinclair/typebox": "0.34.6",
      "@radix-ui/themes": "3.1.4"
    }
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  }
}
